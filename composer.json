{
  "name": "a-yasui/laravel-faultline",
  "type": "library",
  "require": {
    "php": ">=5.4",
    "illuminate/support": "^4.0|^5.0",
    "faultline/faultline": "^0.6.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8.35 || ^5.7 || ^6.0",
    "squizlabs/php_codesniffer": "^2.5.1",
    "monolog/monolog": "^1.22"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "a.yasui",
      "email": "a.yasui@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Faultline\\Laravel\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Faultline\\Laravel\\FaultlineServiceProvider"
      ],
      "aliases": {
        "Faultline": "Faultline\\Laravel\\Facades\\Faultline"
      }
    }
  }
